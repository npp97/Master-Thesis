function dx = dxdt(t,x,p,u, mStruct)
  
   kplus = p( mStruct.p.kplus );
   kminus = p( mStruct.p.kminus );
   k2 = p( mStruct.p.k2 );
   A = x( mStruct.s.A );
   AE = x( mStruct.s.AE );
   E = x( mStruct.s.E );
   P = x( mStruct.s.P );

   dx( mStruct.s.A ) = - p1 * x1 + p2 * x2;
   dx( mStruct.s.AE ) = + kplus * A * E -;
   dx( mStruct.s.E ) = k2 * AE - kplus * A * E + kminus AE;
   dx( mStruct.s.P ) = k2 * AE;
   
   dx = dx(:);
   
